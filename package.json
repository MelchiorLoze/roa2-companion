{
  "version": "2.1.0",
  "name": "roa2-companion",
  "private": true,
  "author": {
    "name": "Melchior Loz√©",
    "email": "melchior.loze@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MelchiorLoze/roa2-companion"
  },
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "reset": "expo prebuild --clean",
    "test": "jest --randomize",
    "test:dev": "jest --randomize --watchAll",
    "test:update-snapshots": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --randomize",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "expo lint",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --strict --noEmit"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg|@shopify/react-native-skia)/"
    ],
    "testEnvironment": "@shopify/react-native-skia/jestEnv.js",
    "setupFiles": [
      "react-native-unistyles/mocks",
      "<rootDir>/src/unistyles.ts"
    ],
    "setupFilesAfterEnv": [
      "@shopify/react-native-skia/jestSetup.js",
      "<rootDir>/src/jest.setup.ts",
      "<rootDir>/src/jest.mock.ts"
    ],
    "clearMocks": true
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@shopify/react-native-skia": "2.2.21",
    "@tanstack/react-query": "^5.83.0",
    "country-flag-icons": "^1.5.21",
    "expo": "^54.0.32",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.9",
    "expo-image": "~3.0.11",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-navigation-bar": "~5.0.10",
    "expo-router": "~6.0.22",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "fast-xml-parser": "^5.2.5",
    "luxon": "^3.7.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gifted-charts": "^1.4.62",
    "react-native-nitro-modules": "0.33.2",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-unistyles": "^3.0.22",
    "react-native-worklets": "0.5.1",
    "type-fest": "^5.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@react-native-community/cli": "^17.0.0",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@testing-library/react-native": "^13.2.0",
    "@types/country-flag-icons": "^1.2.2",
    "@types/fetch-mock": "^7.3.8",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.6.2",
    "@types/react": "19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.31.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-jest": "^29.12.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "expo-dev-client": "~6.0.20",
    "fetch-mock": "^12.5.3",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.13",
    "jest-fail-on-console": "^3.3.1",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
