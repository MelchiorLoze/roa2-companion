{
  "name": "roa2-companion",
  "main": "./src/index.ts",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "reset": "expo prebuild --clean",
    "test": "jest --randomize",
    "test:dev": "jest --randomize --watchAll",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --randomize",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "expo lint",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --strict --noEmit"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "@shopify/react-native-skia/jestSetup.js",
      "react-native-unistyles/mocks",
      "<rootDir>/src/unistyles.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest.setup.ts",
      "<rootDir>/src/jest.mock.ts"
    ],
    "clearMocks": true
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@shopify/react-native-skia": "1.5.0",
    "@tanstack/react-query": "^5.80.6",
    "expo": "~52.0.46",
    "expo-font": "~13.0.4",
    "expo-image": "~2.0.7",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.21",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "^0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "fast-xml-parser": "^5.2.3",
    "luxon": "^3.6.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gifted-charts": "^1.4.61",
    "react-native-nitro-modules": "^0.26.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-unistyles": "^3.0.0-rc.4"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@react-native-community/cli": "^17.0.0",
    "@tanstack/eslint-plugin-query": "^5.78.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/fetch-mock": "^7.3.8",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.6.2",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~8.0.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "expo-dev-client": "~5.0.19",
    "fetch-mock": "^12.5.2",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.6",
    "jest-fail-on-console": "^3.3.1",
    "prettier": "^3.5.3",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.8.3"
  }
