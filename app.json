{
  "expo": {
    "name": "roa2-companion",
    "slug": "roa2-companion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cp0-mand4le.roa2companion"
    },
    "android": {
      "package": "com.cp0_mand4le.roa2companion"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/icon.png",
          "imageWidth": 200,
          "backgroundColor": "#190D45"
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./src/assets/fonts"]
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
