{
  "expo": {
    "name": "roa2-companion",
    "slug": "roa2-companion",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "roa2-companion",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#0E0B2A",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cp0-mand4le.roa2companion"
    },
    "android": {
      "versionCode": 4,
      "package": "com.cp0_mand4le.roa2companion",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/icon.png",
          "imageWidth": 200,
          "backgroundColor": "#190D45"
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./src/assets/fonts"]
        }
      ],
      "expo-secure-store",
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "parentTheme": "Default",
            "enforceNavigationBarContrast": false
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
